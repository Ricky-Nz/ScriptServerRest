{
  "name": "Report",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "tags": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date",
      "required": true
    },
    "platform": {
      "type": "string",
      "required": true
    },
    "platformVersion": {
      "type": "string",
      "required": true
    },
    "scripts": {
      "type": ["object"],
      "required": true
    },
    "packageName": {
      "type": "string"
    },
    "packageDate": {
      "type": "string"
    },
    "pacakgeDescription": {
      "type": "string"
    },
    "packagePath": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "tester": {
      "type": "belongsTo",
      "model": "Tester",
      "foreignKey": "testerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": [
        "create",
        "deleteById",
        "findById",
        "updateAttributes"
      ]
    }
  ],
  "methods": {}
}
